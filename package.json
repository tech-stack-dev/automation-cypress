{
  "name": "automation-cypress",
  "version": "0.0.1",
  "description": "Boilerplate for automation framework based on Cypress",
  "main": "index.js",
  "scripts": {
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
